<Window x:Class="SmartHome.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:local="clr-namespace:SmartHome"
        Title="Humidité" Height="350" Width="525">

    <Window.DataContext>
        <local:MainViewModel />
    </Window.DataContext>

    <Grid>
        <oxy:Plot Title="{Binding Humidité.Title}">
            <oxy:Plot.Series>
                <oxy:LineSeries ItemsSource="{Binding Humidité.Points}" />
            </oxy:Plot.Series>
        </oxy:Plot>
    </Grid>
</Window>
